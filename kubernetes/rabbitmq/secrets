echo "$(tput setaf 2)Usuario:$(tput sgr0) $(kubectl get secrets -n dedicated rabbitmq-dedicated-default-user -o jsonpath="{.data.username}" | base64 -d)"
echo "$(tput setaf 2)Senha:$(tput sgr0) $(kubectl get secrets -n dedicated rabbitmq-dedicated-default-user -o jsonpath="{.data.password}" | base64 -d)"